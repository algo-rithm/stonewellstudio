<script>
	$("document").ready(function($){
		var nav = $('.inventory-menu');
		
		
		$(window).scroll(function(){
			if ($(this).scrollTop() > 400){
				nav.fadeIn('slow');
			} else {
				nav.fadeOut("slow");
			}
		});
	});
</script>
